{
  "name": "proxy-orchestrator",
  "version": "0.0.3",
  "description": "Simple proxy library",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "lint": "./node_modules/.bin/jshint . --config .jshintrc",
    "test": "./node_modules/.bin/mocha tests/mocha.js --reporter spec --check-leaks",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha tests/mocha.js -- --reporter spec",
    "check": "./node_modules/.bin/istanbul check-coverage --statement 100 --branch 100 --function 100 --lines 100",
    "coverage": "npm run-script cover && npm run-script check"
  },
  "homepage": "https://github.com/dgofman/proxy-orchestrator.git",
  "author": "David Gofman <dgofman@gmail.com>",
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dgofman/proxy-orchestrator.git"
  },
  "dependencies": {
    "debug": "~2.2.0",
    "querystring": "~0.2.0"
  },
  "devDependencies": {
    "form-data": "~1.0.0-rc3",
    "assert": "~1.3.0",
    "mocha": "~2.3.3",
    "istanbul": "~0.4.0",
    "jshint": "~2.8.0"
  },
  "license": "MIT"
}