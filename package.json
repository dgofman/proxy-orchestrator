{
  "name": "proxy-orchestrator",
  "version": "0.0.8",
  "description": "Simple proxy library",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "lint": "jshint . --config .jshintrc",
    "test": "mocha tests/mocha.js --reporter spec --check-leaks",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha tests/mocha.js -- --reporter spec",
    "check": "istanbul check-coverage --statement 100 --branch 100 --function 100 --lines 100",
    "coverage": "npm run-script cover && npm run-script check"
  },
  "homepage": "https://github.com/dgofman/proxy-orchestrator.git",
  "author": "David Gofman <dgofman@gmail.com>",
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dgofman/proxy-orchestrator.git"
  },
  "dependencies": {
    "debug": "~2.2.0",
    "querystring": "~0.2.0"
  },
  "devDependencies": {
    "form-data": "~1.0.0-rc3",
    "assert": "~1.3.0",
    "mocha": "~2.4.4",
    "istanbul": "~0.4.0",
    "jshint": "~2.9.1"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dgofman/proxy-orchestrator/issues"
  },
  "directories": {
    "test": "tests"
  },
  "keywords": [
    "proxy",
    "orchestrator",
    "rest",
    "nodejs",
    "http",
    "https"
  ]
}
